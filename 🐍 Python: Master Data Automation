# Data Cleaning for Master Records
import pandas as pd
df = pd.read_excel("doctor_records.xlsx")
df['Hospital_Name'] = df['Hospital_Name'].str.strip().str.title()
df.to_csv("miq_export.txt", sep='\t', index=False)
